{
     "name": "@muhgholy/next-drive",
     "version": "1.4.1",
     "description": "File storage and management for Next.js applications",
     "type": "module",
     "exports": {
          "./server": {
               "types": "./dist/server/index.d.ts",
               "import": "./dist/server/index.js"
          },
          "./server/express": {
               "types": "./dist/server/express.d.ts",
               "import": "./dist/server/express.js"
          },
          "./client": {
               "types": "./dist/client/index.d.ts",
               "import": "./dist/client/index.js"
          },
          "./schemas": {
               "types": "./dist/schemas.d.ts",
               "import": "./dist/schemas.js"
          },
          "./client/styles.css": "./dist/client/styles.css"
     },
     "files": [
          "dist"
     ],
     "scripts": {
          "build": "tsup && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
          "dev": "tsup --watch",
          "lint": "eslint \"src/**/*.{ts,tsx}\"",
          "typecheck": "tsc --noEmit",
          "test": "vitest",
          "test:coverage": "vitest --coverage",
          "test:integration": "vitest -c vitest.integration.config.ts"
     },
     "peerDependencies": {
          "mongoose": ">=7.0.0",
          "next": ">=14.0.0",
          "react": ">=18.0.0",
          "react-dom": ">=18.0.0"
     },
     "dependencies": {
          "@dnd-kit/core": "^6.3.1",
          "@dnd-kit/sortable": "^10.0.0",
          "@dnd-kit/utilities": "^3.2.2",
          "@radix-ui/react-alert-dialog": "^1.1.15",
          "@radix-ui/react-context-menu": "^2.2.16",
          "@radix-ui/react-dialog": "^1.1.15",
          "@radix-ui/react-dropdown-menu": "^2.1.16",
          "@radix-ui/react-label": "^2.1.8",
          "@radix-ui/react-progress": "^1.1.8",
          "@radix-ui/react-separator": "^1.1.8",
          "@radix-ui/react-slot": "^1.2.4",
          "@radix-ui/react-tooltip": "^1.2.8",
          "class-variance-authority": "^0.7.1",
          "clsx": "^2.1.1",
          "date-fns": "^4.1.0",
          "fluent-ffmpeg": "^2.1.2",
          "formidable": "^3.5.4",
          "googleapis": "^169.0.0",
          "lucide-react": "^0.562.0",
          "sharp": "^0.33.0",
          "tailwind-merge": "^3.4.0",
          "zod": "^3.22.0"
     },
     "devDependencies": {
          "@eslint/js": "^9.39.2",
          "@semantic-release/changelog": "^6.0.3",
          "@semantic-release/exec": "^7.1.0",
          "@semantic-release/git": "^10.0.1",
          "@types/express": "^5.0.6",
          "@types/fluent-ffmpeg": "^2.1.21",
          "@types/formidable": "^3.4.6",
          "@types/node": "^20.0.0",
          "@types/react": "^18.0.0",
          "@types/react-dom": "^18.0.0",
          "@vitest/coverage-v8": "^4.0.16",
          "eslint": "^8.0.0",
          "jsdom": "^27.3.0",
          "mongoose": ">=7.0.0",
          "semantic-release": "^24.2.9",
          "tsc-alias": "^1.8.16",
          "tsup": "^8.0.0",
          "typescript": "^5.0.0",
          "typescript-eslint": "^8.50.1",
          "vitest": "^4.0.16"
     },
     "keywords": [
          "next.js",
          "file-storage",
          "drive",
          "upload",
          "file-management"
     ],
     "publishConfig": {
          "access": "public"
     },
     "license": "MIT",
     "repository": {
          "type": "git",
          "url": "https://github.com/muhgholy/next-drive"
     }
}
